<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.homeservicefinal">
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.SEND_SMS"/>
    <uses-permission android:name="android.permission.VIBRATE"/>



    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/w1"
        android:label="@string/app_name"
        android:roundIcon="@drawable/w1"
        android:supportsRtl="true"
        android:theme="@style/Theme.Homeservicefinal">
        <activity
            android:name=".adminmenu"
            android:exported="false"
            android:icon="@drawable/w1"
            android:label="ADMIN MENU"/>
        <activity
            android:name=".technicianadddata"
            android:exported="false"
            android:icon="@drawable/w1"
            android:label="TECHNICIAN DATA"/>
        <activity
            android:name=".electricianadddata"
            android:exported="false"
            android:icon="@drawable/w1"
            android:label="ELECTRICIAN DATA"/>
        <activity
            android:name=".plumberadddata"
            android:exported="false"
            android:icon="@drawable/w1"
            android:label="PLUMBER DATA"/>

        <activity
            android:name=".adminlogin"
            android:exported="false"
            android:icon="@drawable/w1"
            android:label="ADMIN LOGIN"/>
        <activity
            android:name=".technician_page"
            android:exported="false"
            android:icon="@drawable/w1"
            android:label="TECHNICIAN"/>
        <activity
            android:name=".electrician_page"
            android:exported="false"
            android:icon="@drawable/w1"
            android:label="ELECTRICIAN"
            android:parentActivityName=".menu"/>
        <activity
            android:name=".plumber_page"
            android:exported="false"
            android:icon="@drawable/w1"
            android:label="PLUMBER"/>
        <activity
            android:name=".menu"
            android:exported="false"
            android:icon="@drawable/w1"
            android:label="WELCOME" />
        <activity
            android:name=".Login"
            android:exported="true"
            android:icon="@drawable/w1"
            android:label="LOGIN"
            android:parentActivityName=".Signup"/>
        <activity
            android:name=".Signup"
            android:exported="true"
            android:icon="@drawable/w1"
            android:label="SIGNUP"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:icon="@drawable/w1"
            android:label="HOUSEJOY">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>